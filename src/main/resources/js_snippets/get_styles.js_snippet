function getStyles(arg) {
    let v = window.getComputedStyle(arg, null), d = [];
    for (let q in v) {
        if (q === undefined || v[q] === null) continue;
        if (!isNaN(parseFloat(q)) && isFinite(q)) continue;
        d.push({name: q, value: v[q]});
    }

    return d;
}

return getStyles(arguments[0]);